#include<stdio.h>
#include<string.h>
int main()
{
	char str1[100],str2[100];
	printf("\nEnter username:");
	scanf("%s",&str1);
	printf("\nRe enter username:");
	scanf("%s",&str2);
	if (strcmp(str1,str2)==0)
	{
	printf("\nUsername is Valid!");
	}
	else
	{
	printf("\nUsername is Invalid!");
	}
	return 0;
}
